{
  "name": "react-class-container",
  "version": "1.0.4",
  "description": "Manage your react containers as es6 classes",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "tslint src/**",
    "doc": "typedoc --mode file --excludePrivate excludeNotExported --out docs src",
    "prepublish": "npm run build && npm run lint",
    "postpublish": "rm -rf ./*.{js,js.map,d.ts}"
  },
  "keywords": [
    "react",
    "redux",
    "container",
    "class",
    "es6",
    "decorator"
  ],
  "author": "o.herrmann92@gmail.com",
  "license": "ISC",
  "peerDependencies": {
    "react": ">=15.x.x",
    "redux": ">=3.x.x"
  },
  "devDependencies": {
    "@types/react": "16.0.18",
    "@types/redux": "3.6.0",
    "react": "16.4.1",
    "redux": "4.0.0",
    "typedoc": "0.11.1",
    "typescript": "2.7.2"
  },
  "dependencies": {
    "@types/prop-types": "15.5.2",
    "prop-types": "15.6.0",
    "tslint": "5.8.0"
  }
}
