{
  "name": "react-class-container",
  "version": "2.2.0",
  "description": "Manage your react containers as es6 classes",
  "main": "lib/src/index.js",
  "typings": "lib/src/index.d.ts",
  "scripts": {
    "test": "ava lib/test/**/*.js",
    "build": "tsc",
    "lint": "tslint src/**",
    "doc": "typedoc --mode file --excludePrivate excludeNotExported --out docs src",
    "prepublish": "npm run build && npm run lint",
    "postpublish": "rm -rf ./*.{js,js.map,d.ts}"
  },
  "keywords": [
    "react",
    "redux",
    "container",
    "class",
    "es6",
    "decorator"
  ],
  "author": "o.herrmann92@gmail.com",
  "license": "ISC",
  "peerDependencies": {
    "react": ">=15.x.x",
    "redux": ">=3.x.x"
  },
  "devDependencies": {
    "@types/enzyme": "3.1.11",
    "@types/enzyme-adapter-react-16": "1.0.2",
    "@types/node": "12.6.8",
    "@types/react": "16.0.18",
    "@types/redux": "3.6.0",
    "ava": "2.2.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "redux": "4.0.0",
    "tslint": "5.8.0",
    "typedoc": "0.14.2",
    "typescript": "3.7.5"
  },
  "dependencies": {
    "@types/prop-types": "15.5.2",
    "prop-types": "15.6.0"
  }
}
